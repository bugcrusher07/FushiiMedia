// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model FormSubmission {
  id        String   @id @default(cuid())
  name      String
  email     String
  message   String
  phone     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}